<html>
<%- include('header', {logged: true, firstName: false}); %>

<div id="add-product-form" class="container">
	<div class="row">
		<div class="col-sm-12 title">
			<h2>Agregar Consignatario</h2>
		</div>
	</div>
	<div class="row">
		<div class="col-sm-6 col-md-4 col-md-offset-4">
			<form class='form-add-product' action='/add_consignee' method='POST'>
				<input type='text', name='consigneename', class='form-control', placeholder='Nombre del Consignatario' required autofocus>
				<input type='text', name='consigneerut', class='form-control', placeholder='RUT del Consignatario' required>
				<input type='text', name='consigneeaddress', class='form-control', placeholder='Direccion del Consignatario' required>
				<button class='btn btn-lg btn-primary btn-block' type='submit'>Agregar Consignatario</button>
			<span class="clearfix"></span>
			</form>
		</div>
	</div>
</div>
</html>